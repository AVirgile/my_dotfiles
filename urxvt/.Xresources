! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6
 
! black
*.color0:       #1d1f21
*.color8:       #969896
 
! red
*.color1:       #cc342b
*.color9:       #cc342b
 
! green
*.color2:       #198844
*.color10:      #198844
 
! yellow
*.color3:       #fba922
*.color11:      #fba922
 
! blue
*.color4:       #3971ed
*.color12:      #3971ed
 
! magenta
*.color5:       #a36ac7
*.color13:      #a36ac7
 
! cyan
*.color6:       #0f73fb
*.color14:      #0f73fb
 
! white
*.color7:       #c5c8c6
*.color15:      #ffffff

! font
URxvt.font: xft:MesloLGS NF:size=12
! remove ugly bar
URxvt.scrollBar: false
! native transparency
URxvt*inheritPixmap: true
URxvt*transparent: true
URxvt*shading: 50
!plugins
URxvt.perl-ext-common: fullscreen
URxvt.keysym.F11: perl:fullscreen:switch
